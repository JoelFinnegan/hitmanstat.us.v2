@{
    ViewData["Title"] = "Events";
}

<div class="alert alert-header" role="alert">
    <div class="text-center">
        <h4 class="alert-heading">LATEST DOWN OR MAINTENANCE EVENTS REPORTED</h4>
        <span class="separator"></span>
        <p class="mb-0">Since 7 days ago &nbsp; | &nbsp; <a href="#">Show last month</a></p>
    </div>
</div>

<table class="table table-striped table-light">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Date</th>
            <th scope="col">Relative Time</th>
            <th scope="col">Service</th>
            <th scope="col">Status</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td class="text-center" colspan="4">No event occurred.</td>
        </tr>
    </tbody>
</table>

@section Scripts {
    <script src="~/js/jquery-bootstrap.min.js" asp-append-version="true"></script>
}